pkg complex_number ifc {

    type complex_number_type;

    /*
     * accessors
     */
    accessor imaginary(c: complex_number_type) $ float;
    accessor real(c: complex_number_type) $ float;
    /*
     * operations
     */
    func "-"(c1, c2: complex_number_type) $ complex_number_type;
    func "+"(c1, c2: complex_number_type) $ complex_number_type;

};
